# Penjumlahan
a = 5
b = 3
hasil_penjumlahan = a + b
print("Hasil Penjumlahan:", hasil_penjumlahan)

# Pengurangan
hasil_pengurangan = a - b
print("Hasil Pengurangan:", hasil_pengurangan)

# Perkalian
hasil_perkalian = a * b
print("Hasil Perkalian:", hasil_perkalian)

# Pembagian
hasil_pembagian = a / b
print("Hasil Pembagian:", hasil_pembagian)
